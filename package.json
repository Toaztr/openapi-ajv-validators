{
  "name": "@toaztr/openapi-ajv-validators",
  "version": "1.2.0",
  "description": "OpenAPI validators generator",
  "scripts": {
    "build": "tsc",
    "sync": "git remote -v | grep upstream || git remote add upstream git@github.com:Toaztr/openapi-ajv-validators.git && git checkout master && git fetch upstream master && git rebase upstream/master && git push origin"
  },
  "bin": {
    "openapi-ajv-validators": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Toaztr/openapi-ajv-validators.git"
  },
  "keywords": [
    "api",
    "fintech",
    "toaztr",
    "ajv"
  ],
  "author": "Toaztr SAS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Toaztr/openapi-ajv-validators/issues"
  },
  "homepage": "https://github.com/Toaztr/openapi-ajv-validators#readme",
  "devDependencies": {
    "@types/node": "16.4.13",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "ajv": "8.17.1",
    "ajv-formats": "2.1.1",
    "kleur": "4.1.4",
    "meow": "9.0.0"
  }
}
